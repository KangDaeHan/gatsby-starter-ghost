{
    "name": "fizzy",
    "description": "A tasty blogging theme for Ghost.",
    "version": "1.3.2",
    "author": {
        "name": "",
        "email": ""
    },
    "resolutions": {
      "sharp": "0.27.0"
    },
    "engines": {
        "ghost": ">=3.0.0",
        "ghost-api": "v3"
    },
    "keywords": [
        "ghost",
        "theme",
        "ghost-theme"
    ],
    "main": "n/a",
    "scripts": {
      "serve": "gatsby build && NODE_ENV=production gatsby serve",
      "build": "gatsby build",
      "dev": "gatsby develop",
      "lint": "eslint . --ext .js --cache",
      "test": "echo \"Error: no test specified\" && exit 1"
    },
    "devDependencies": {
      "babel-eslint": "10.1.0",
      "eslint": "7.17.0",
      "eslint-plugin-ghost": "2.0.0",
      "eslint-plugin-react": "7.22.0"
    },
    "dependencies": {
      "@tryghost/helpers": "1.1.36",
      "@tryghost/helpers-gatsby": "1.0.41",
      "cheerio": "1.0.0-rc.3",
      "gatsby": "2.30.1",
      "gatsby-awesome-pagination": "0.3.6",
      "gatsby-image": "2.9.0",
      "gatsby-plugin-advanced-sitemap": "1.5.6",
      "gatsby-plugin-catch-links": "2.8.0",
      "gatsby-plugin-feed": "2.11.0",
      "gatsby-plugin-force-trailing-slashes": "1.0.4",
      "gatsby-plugin-manifest": "2.10.0",
      "gatsby-plugin-offline": "3.8.0",
      "gatsby-plugin-react-helmet": "3.8.0",
      "gatsby-plugin-sharp": "2.12.0",
      "gatsby-source-filesystem": "2.9.0",
      "gatsby-source-ghost": "4.2.2",
      "gatsby-transformer-sharp": "2.10.0",
      "lodash": "4.17.20",
      "react": "17.0.1",
      "react-dom": "17.0.1",
      "react-helmet": "6.1.0"
    },
    "config": {
        "posts_per_page": 8,
        "image_sizes": {
            "icon": {
                "width": 64
            },
            "small": {
                "width": 300
            },
            "medium": {
                "width": 600
            },
            "large": {
                "width": 900
            },
            "wide": {
                "width": 1200
            }
        }
    }
}
